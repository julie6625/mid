package com.fju.mid;
import java.util.Scanner;
public class Numbers {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int sc1 = scanner.nextInt();
		for(int i = 1; i <= sc1; i++){
			for(int a = 1; a<i; a++){
				System.out.print("#");
			}
			System.out.print(i+" ");
		}
	}

}
